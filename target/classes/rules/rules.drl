package rules

import com.schneider.q2o2.quote.pricing.dto.PriceDto
import com.schneider.q2o2.quote.pricing.dto.QuoteDto

rule "Price for GE"
	when 
		quoteObject: QuoteDto(oppurtunityName=="GE")
	then
		quoteObject.setPrice(100.10);
	end
rule "Price for ABB"
	when 
		quoteObject: QuoteDto(oppurtunityName=="ABB")
	then
		quoteObject.setPrice(200.10);
	end
